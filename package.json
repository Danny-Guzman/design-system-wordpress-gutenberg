{
  "name": "design-system-wordpress-gutenberg",
  "version": "1.2.1",
  "description": "Integrates the State of California Design System into WordPress.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cagov/design-system-wordpress-gutenberg.git"
  },
  "keywords": [
